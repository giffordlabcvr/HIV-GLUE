  module hivPhyloUtility
    reroot-phylogeny -i trees/HIV-1_AL_UNCONSTRAINED.tre NEWICK_BOOTSTRAPS -m -o trees/HIV-1_AL_UNCONSTRAINED_midpoint.tre NEWICK_BOOTSTRAPS
    exit
    