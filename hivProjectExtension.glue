# Build HIV project extension
project hiv

  # Delete any extension build items from previous build
  delete source --whereClause "name like 'ncbi-curated%'" 

  # Import reference sequences from GenBank XML files 
  run file glue/extension/hivCuratedImportSources.glue  

  # Populate fields using tabular file data
  run file glue/extension/hivCuratedLoadTabularData.glue  
 
  # Import alignments
  run file glue/extension/hivCuratedAlignmentTree.glue

  validate
  